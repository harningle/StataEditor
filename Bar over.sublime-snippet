<snippet>
	<content><![CDATA[
graph bar ${1:y1} ${2:y1} ${3:y3}
  , over(decade)
    bar(1, col("255 223 0"))
    bar(2, col("0 140 69"))
    bar(3, col("45 95 161"))
    legend(lab(1 "")
           lab(2 "")
           lab(3 "")
           rows(3) cols(1) pos(11) ring(0) region(lw(thin) lc(black)))
    xsize(16) ysize(9) ;
#delimit cr
]]></content>
	<tabTrigger>barover</tabTrigger>
	<scope>source.stata</scope>scope>
	<description>Bar over</description>description
</snippet>
